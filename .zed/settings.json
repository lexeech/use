{
  "auto_update": true,
  "bottom_dock_layout": "left_aligned",
  "unstable.ui_density": "compact",
  "use_system_prompts": false,
  "show_user_picture": false,
  "hide_mouse": "on_typing_and_movement",
  "go_to_definition_fallback": "none",
  "cursor_shape": "block",
  "cursor_blink": false,
  "multi_cursor_modifier": "alt",
  "use_smartcase_search": true,
  "selection_highlight_debounce": 250,
  "lsp_highlight_debounce": 250,
  "hover_popover_delay": 500,
  "base_keymap": "None",
  "restore_on_startup": "last_session",
  "preview_tabs": {
    "enabled": false
  },
  "project_panel": {
    "default_width": 480,
    "dock": "left",
    "file_icons": false,
    "folder_icons": false,
    "indent_size": 10,
    "entry_spacing": "standard",
    "button": true
  },
  "file_finder": {
    "skip_focus_for_active_in_search": false,
    "modal_max_width": "large"
  },
  "features": {
    "copilot": false,
    "edit_prediction_provider": "none"
  },
  "theme": "Estafette Dark",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": false,
  "ui_font_size": 15,
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "buffer_font_size": 14,
  "buffer_line_height": "comfortable",
  "buffer_font_family": "Pragmata Pro",
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": false
  },
  "tabs": {
    "show_diagnostics": "errors",
    "close_position": "right",
    "file_icons": false,
    "git_status": true,
    "activate_on_close": null,
    "show_close_button": "hover"
  },
  "collaboration_panel": {
    "enabled": false,
    "button": false
  },
  "outline_panel": {
    "enabled": false,
    "button": false
  },
  "terminal": {
    "enabled": false,
    "button": false,
    "font_size": 12
  },
  "agent": {
    "version": "1",
    "enabled": false,
    "button": false
  },
  "notification_panel": {
    "enabled": false,
    "button": false
  },
  "confirm_quit": true,
  "remove_trailing_whitespace_on_save": true,
  "when_closing_with_no_tabs": "keep_window_open",
  "gutter": {
    "line_numbers": true,
    "runnables": false,
    "code_actions": false,
    "folds": false,
    "breakpoints": false
  },
  "soft_wrap": "editor_width",
  "wrap_guides": [
    125,
    136
  ],
  "git": {
    "hunk_style": "staged_hollow",
    "gutter_debounce": 500,
    "inline_blame": {
      "enabled": false,
      "show_commit_summary": true
    }
  },
  "git_panel": {
    "button": true
  },
  "title_bar": {
    "show_branch_icon": false,
    "show_user_picture": false
  },
  "scroll_sensitivity": 2,
  "formatter": "language_server",
  "languages": {
    "JavaScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    },
    "TypeScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    },
    "SCSS": {
      "Emmet": {
        "CSS": true
      }
    },
    "Python": {
      "format_on_save": "on",
      "language_servers": [
        "pyright",
        "ruff"
      ],
      "formatter": [
        {
          "language_server": {
            "name": "ruff"
          }
        }
      ]
    }
  },
  "lsp": {
    "ruff": {
      "initialization_options": {
        "settings": {
          "configuration": "ruff.toml"
        }
      }
    },
    "pyright": {
      "settings": {
        "python": {
          "pythonPath": ".venv/bin/python"
        }
      }
    }
  },
  "inlay_hints": {
    "enabled": false
  },
  "experimental.theme_overrides": {
    "hint": "#737c7c"
  }
}
