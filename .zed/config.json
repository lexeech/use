{
  "base_keymap": "None",
  "preview_tabs": {
    "enabled": false
  },
  "project_panel": {
    "default_width": 480,
    "dock": "left",
    "file_icons": false,
    "folder_icons": false,
    "indent_size": 10,
    "entry_spacing": "comfortable",
    "button": false
  },
  "features": {
    "copilot": false,
    "inline_completion_provider": "none"
  },
  "theme": "Fleet Dark", // from Fleet Themes
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": false,
  "ui_font_size": 15,
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "buffer_font_size": 14.0,
  "preferred_line_length": 80,
  "buffer_line_height": "comfortable",
  "buffer_font_family": "Pragmata Pro",
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": false
  },
  "tabs": {
    "show_diagnostics": "errors",
    "close_position": "right",
    "file_icons": false,
    "git_status": true,
    "activate_on_close": null,
    "always_show_close_button": false
  },
  "collaboration_panel": {
    "enabled": false,
    "button": false
  },
  "outline_panel": {
    "enabled": false,
    "button": false
  },
  "terminal": {
    "enabled": false,
    "button": false
  },
  "assistant": {
    "version": "1",
    "enabled": false,
    "button": false
  },
  "notification_panel": {
    "enabled": false,
    "button": false
  },
  "confirm_quit": true,
  "remove_trailing_whitespace_on_save": true,
  "when_closing_with_no_tabs": "keep_window_open",
  "gutter": {
    "runnables": false,
    "code_actions": false
  },
  "soft_wrap": "editor_width",
  "wrap_guides": [125, 136],
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "experimental.theme_overrides": {
    "text": "#fafafa",
    "editor.background": "#1d1f20",
    "panel.background": "#252526",
    "tab.active_background": "#1d1f20",
    "tab.inactive_background": "#2d2d2d",
    "title_bar.background": "#333333",
    "tab_bar.background": "#252526",
    "terminal.background": "#1d1f20",
    "toolbar.background": "#1d1f20",
    "status_bar.background": "#333333",
    "editor.gutter.background": "#1d1f20",
    "editor.active_line.background": "#3366ff11",
    "players": [{ "cursor": "#fafafa" }],
    "syntax": {
      "operator": { "color": "#8ECBFE" },
      "type": { "color": "#F8FFB6" },
      "boolean": { "color": "#99CC89" },
      "attribute": { "color": "#C7C5FF" },
      "string": { "color": "#A8F13E" },
      "variable": { "color": "#C7C5FF" },
      "property": { "color": "#C7C5FF" },
      "constant": { "color": "#C7C5FF" },
      "function": { "color": "#DBD2A7" },
      "function.definition": {
        "color": "#DBD2A7",
        "font_style": "normal",
        "font_weight": 600
      },
      "keyword": { "color": "#8ECBFE", "font_style": "italic" }
    }
  }
}
